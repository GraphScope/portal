{
  "engines": {
    "node": "^18.19.1",
    "pnpm": "^10||^9"
  },
  "packageManager": "pnpm@9.15.6",
  "scripts": {
    "prev": "pnpm i --force && rm -rf package-lock.json",
    "start": "turbo run start",
    "build": "turbo run build --no-cache",
    "build:portal": "npm run build && cd packages/studio-website && npm run build:site",
    "build:portal:single": "npm run build && cd packages/studio-website && npm run build:site:single",
    "build:docs:interactive": "npm run build && cd docs/interactive && npm run build:site",
    "build:docs:portal": "npm run build && dumi build",
    "docs": "dumi dev",
    "version": "changeset version",
    "publish": "npm run build && pnpm changeset publish -r",
    "check:publish": "npm run build && pnpm changeset publish -r --dry-run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "pnpm": {
    "overrides": {
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "@types/react": "18.2.0",
      "@types/react-dom": "18.2.0",
      "@ant-design/colors": "^7.1.0",
      "antd": "^5.22.2",
      "@antv/g2": "^5.2.8"
    }
  },
  "dependencies": {
    "@ant-design/colors": "^7.1.0",
    "antd": "^5.22.2",
    "@ant-design/icons": "^5.2.6",
    "@uiw/codemirror-themes": "^4.21.24",
    "buffer": "^6.0.3",
    "buffer-browserify": "^0.2.5",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.10",
    "lodash": "4.17.21",
    "node-fetch": "^3.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "use-immer": "^0.9.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-react": "^7.24.7",
    "@changesets/cli": "^2.27.7",
    "@types/node": "latest",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.8.1",
    "dumi": "^2.2.17",
    "dumi-theme-antd": "^0.4.2",
    "father": "^4.4.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.3",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "turbo": "^1.8.3",
    "typescript": "^5.1.6",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": []
}
