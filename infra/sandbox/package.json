{
  "name": "@graphscope/sandbox",
  "version": "0.1.0",
  "description": "Secure sandbox environment for executing code in isolated containers",
  "main": "dist/lib/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/lib/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "scripts": {
    "start": "concurrently \"tsc --watch -p tsconfig.build.esm.json\" \"tsc --watch -p tsconfig.build.cjs.json\"",
    "build": "tsc  -p tsconfig.build.esm.json && tsc  -p tsconfig.build.cjs.json ",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@graphscope/logger": "workspace:*",
    "@types/http-proxy": "^1.17.16",
    "cors": "^2.8.5",
    "dockerode": "^3.3.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-proxy": "^1.18.1",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.11.5",
    "@types/archiver": "^6.0.3",
    "@types/cors": "^2.8.17",
    "@types/dockerode": "^3.3.38",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "archiver": "^7.0.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "tar-stream": "^3.1.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
