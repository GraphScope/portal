{
  "name": "@graphscope/studio-query",
  "version": "0.1.0",
  "description": "A data querying module in GraphStudio, supporting Cypher and Gremlin statements",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/GraphScope/graphscope-components.git"
  },
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "start": "run-p build:cjs:watch build:esm:watch",
    "build:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib",
    "build:esm": "rimraf ./esm && tsc --module ESNext --outDir es",
    "build:cjs:watch": "rimraf ./lib && tsc --module commonjs --outDir lib --watch",
    "build:esm:watch": "rimraf ./esm && tsc --module ESNext --outDir es --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@antv/g2": "5.1.13",
    "@antv/graphin": "latest",
    "@difizen/cofine-editor-core": "^0.0.1-alpha.8",
    "@difizen/cofine-language-cypher": "^0.0.1-alpha.8",
    "@difizen/cofine-language-gremlin": "^0.0.1-alpha.8",
    "@fortawesome/free-solid-svg-icons": "latest",
    "@fortawesome/react-fontawesome": "latest",
    "@graphscope/_test_gremlin_": "^0.1.2",
    "dayjs": "^1.11.10",
    "neo4j-driver": "^5.12.0",
    "re-resizable": "^6.9.11",
    "react-intl": "^6.6.1",
    "react-json-view": "^1.21.3",
    "uuid": "^9.0.1",
    "valtio": "^1.12.1"
  },
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  }
}
