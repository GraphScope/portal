{
  "name": "@graphscope/studio-importor",
  "version": "0.1.0",
  "description": "A data modeling and import module in GraphStudio",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/GraphScope/graphscope-components.git"
  },
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "start": "run-p build:cjs:watch build:esm:watch",
    "build:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib",
    "build:esm": "rimraf ./esm && tsc --module ESNext --outDir es",
    "build:cjs:watch": "rimraf ./lib && tsc --module commonjs --outDir lib --watch",
    "build:esm:watch": "rimraf ./esm && tsc --module ESNext --outDir es --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reactflow": "latest",
    "valtio": "^1.12.1"
  },
  "author": "",
  "license": "ISC"
}
