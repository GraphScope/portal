{
  "scripts": {
    "predev": "rm -rf package-lock.json && npm i",
    "dev": "node index.js --",
    "prestart": "rm -rf package-lock.json && npm i",
    "restart": "npx pm2 delete portal && npm run start --",
    "start": "npx pm2 start index.js --name portal --",
    "logs": "npx pm2 logs",
    "status": "npx pm2 status"
  },
  "dependencies": {
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "pm2": "^5.3.1"
  }
}
